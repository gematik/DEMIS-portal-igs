{
  "name": "portal-igs",
  "version": "1.3.4-snapshot",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4204",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test-local": "ng test --karma-config karma.conf.js",
    "test": "ng test --karma-config karma.single-run.conf.js",
    "test:coverage": "ng test --karma-config karma.coverage.conf.js",
    "clean-cache": "rimraf .angular/cache && echo Cache cleared - next build will be slower but clean",
    "build-fresh": "npm run clean-cache && npm run build",
    "docker": "docker build -t europe-west3-docker.pkg.dev/gematik-all-infra-prod/demis-dev/portal-igs:latest .",
    "build:single-spa:portal-igs": "ng build portal-igs --configuration production",
    "serve:single-spa:portal-igs": "ng s --project portal-igs --disable-host-check --port 4204 --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.18",
